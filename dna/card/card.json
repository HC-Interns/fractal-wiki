{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "http://example.com/card/card.json",
  "type": "object",
  "required": [
    "title",
    "source",
    "content"
  ],
  "properties": {
    "title": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "timestamp": {
      "type": "integer"
    },
    "like": {
      "default": 0,
      "type": "integer"
    },
    "dislike": {
      "default": 0,
      "type": "integer"
    },
    "card_type": {
      "type": "string"
    }
  }
}
